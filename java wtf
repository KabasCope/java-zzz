// and, or, not son operadores que sirven para el if

package javaapplication1;

import java.util.Scanner;

public class JavaApplication1 {


    public static void main(String[] args) {
        
        
        Scanner Lectura = new Scanner (System.in);
        
        System.out.println("Ingrese un valor: ");
        
        int N1 = Lectura.nextInt();
        
        System.out.println("Ingrese el segundo valor; ");
        
        int N2 = Lectura.nextInt();
        
        int Resultado = N1  * N2;
        
        System.out.println("El resultado es: " + Resultado);
        
                if (N1 < N2 ) {    //Si el numero 1 es menor que N2
            System.out.println("El numero mayor es " + N2);
            
        }else if (N1 > N2){ // por otro lado si es N1 mayor a N2
            System.out.println("El numero mayor es " + N1);
            
        }else if (N1 == N2){ //si ambos numeros son iguales
            System.out.println("Ambos numeros son iguales");
        }
    }
}
